<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="150" d:DesignHeight="600"
             x:Class="LSL.Views.Settings.SettingsLeft">
	<StackPanel Width="150">
		<TextBlock DockPanel.Dock="Top" Text="设置" Margin="10,10,0,0" Foreground="Gray"/>
		<Button Content="常规" Classes="left" Command="{Binding RightViewCmd}" CommandParameter="Common" />
		<Button Content="下载" Classes="left" Command="{Binding RightViewCmd}" CommandParameter="DownloadSettings"/>
		<Button Content="网页面板" Classes="left" Command="{Binding RightViewCmd}" CommandParameter="PanelSettings"/>
		<Button Content="个性化" Classes="left" Command="{Binding RightViewCmd}" CommandParameter="StyleSettings"/>
		<Button Content="关于" Classes="left" Command="{Binding RightViewCmd}" CommandParameter="About"/>
	</StackPanel>
</UserControl>
