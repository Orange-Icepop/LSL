<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 xmlns:local="clr-namespace:LSL.Components;assembly=LSL"
             mc:Ignorable="d" d:DesignWidth="1050" d:DesignHeight="600"
             x:Class="LSL.Views.Download.AddServer">
	<ScrollViewer>
		<StackPanel>
			<local:MyCard Text="导入服务器">
				<StackPanel Margin="30,50">
					<local:MyListItem Content="从核心添加服务器" Height="30" Command="{Binding FullViewCmd}" CommandParameter="AddCore"/>
				</StackPanel>
			</local:MyCard>
		</StackPanel>
	</ScrollViewer>
</UserControl>
