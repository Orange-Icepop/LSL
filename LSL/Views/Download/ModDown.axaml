<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 xmlns:local="clr-namespace:LSL.Controls;assembly=LSL"
             mc:Ignorable="d" d:DesignWidth="1050" d:DesignHeight="600"
             x:Class="LSL.Views.Download.ModDown">
	<StackPanel>
		<local:MyCard Text="Mod下载">
			<StackPanel>
				<DockPanel Margin="30,50,0,10">
					<TextBlock DockPanel.Dock="Left" Text="名称" VerticalAlignment="Center" Margin="0,0,10,0"/>
					<TextBox DockPanel.Dock="Left" HorizontalAlignment="Left" VerticalAlignment="Center" Width="400"/>
				</DockPanel>
				<DockPanel Margin="30,0,0,20">
					<Button Classes="rightblue" Width="100" Height="40" Margin="0,0,10,0">搜索</Button>
					<Button Classes="right" Width="100" Height="40" Margin="0,0,10,0">清空条件</Button>
				</DockPanel>
			</StackPanel>
		</local:MyCard>
		<local:MyCard>
